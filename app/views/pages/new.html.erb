<% title "New Page" %>

<%= form_for @page do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.label :query %><br />
    <%= f.text_field :query %>
  </p>
  <p><%= f.submit %></p>
<% end %>


<p><%= link_to "Back to List", pages_path %></p>
